2. style.css
body { // memilih selector tag body
 padding-left: 15px; // mengatur jarak bagian dalam sebelah kiri sebesar 15 pixel
 padding-right: 15px; // mengatur jarak bagian dalam sebelah kanan sebesar 15 pixel
 font-family: Arial, Helvetica, sans-serif; // menggunakan jenis font Arial, Helvetica, 
sans-serif
 margin-bottom: 100px; // mengatur batas bawah sebesar 100 pixel
}
.navi { // memilih selector class .navi
 background: #222; // mengatur warna background
 align-items: center; // mengatur posisi item menjadi tengah
}
a { // memilih selector tag a
 color: black; // mengatur warna teks
 text-decoration: none; // menghilangkan dekorasi pada teks
 padding: 14px 10px; // mengatur jarak bagian dalam sebelah atas dan bawah sebesar 
14 pixel dan sebelah kiri dan kanan sebesar 10 pixel
}
ul { // memilih selector tag ul
 padding: 14px; // mengatur jarak bagian dalam sebesar 14 pixel
}
li { // memilih selector tag li
 list-style: none; // menghilangkan tanda pada item list
 display: inline; // mengatur konten mengalir ke samping (horizontal)
 margin-right: 10px; // mengatur batas kanan sebesar 10 pixel
}
li, a { // memilih selector tag li dan tag a
 background: #222; // mengatur warna background
 color: #fff; // mengatur warna teks
}
li, a:hover { // memilih selector tag li dan tag a ketika di-hover
 background: #4da4ff; // mengatur warna background
 color: #fff; // mengatur warna teks
}
table, p { // memilih selector tag table dan tag p
 margin-left: 15px; // mengatur batas kiri sebesar 15 pixel
}
table { // memilih selector tag table
 border-collapse: collapse; // mengatur garis pembatas menjadi hilang
}
table, th, td { // memilih selector tag table, tag th, dan tag td
 border: 1px solid #999; // mengatur tebal garis pembatas sebesar 1 pixel dan 
warnanya
 padding: 8px 10px; // mengatur jarak bagian dalam sebelah atas dan bawah sebesar 
8 pixel dan sebelah kiri dan kanan sebesar 10 pixel
